{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/generate-thread.ts": {
      "maxDuration": 60
    },
    "api/generate-with-context.ts": {
      "maxDuration": 60
    },
    "api/analyze-topic.ts": {
      "maxDuration": 30
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
} 